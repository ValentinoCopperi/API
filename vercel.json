{ 

    "version": 2,
    "rewrites" : [{"source" : "./src/(.*)" , "destination": "app.ts"}],
    "builds": [
      {
        "src": "./src/app.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/route/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.ts"
      }
    ]
  }
  